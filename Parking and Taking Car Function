#include <Wire.h>
#include <LiquidCrystal_I2C.h>
#include <IRremote.h> // add IR remote library
LiquidCrystal_I2C lcd(0x27, 20, 4); //Change the HEX address

//Setup pins in Channel A
const int MotorPinA = 12;
const int MotorSpeedPinA = 3;
const int MotorBrakePinA = 9;

//Setup pins in Channel B
const int MotorPinB = 13;
const int MotorSpeedPinB = 11;
const int MotorBrakePinB = 8;

//Setup derictions in Channel A & B
const int CW = HIGH;
const int CCW = LOW;

int IR0 = 0;  // sensor pin detect the car outdoor
int IR1 = 0;  // sensor pin detect the car indoor

int recvpin1 = 2; // sensor pin detect the car at the spot 1
int recvpin2 = 4; // sensor pin detect the car at the spot 2
int recvpin3 = 5; // sensor pin detect the car at the spot 3
int recvpin4 = 6; // sensor pin detect the car at the spot 4
int recvpin5 = 7; // sensor pin detect the car at the spot 5
int recvpin6 = 10; // sensor pin detect the car at the spot 6
int Total = 6; //Total number of parking Slots
int Slot; //Parking Slots

// Boolean value to keep track on the parking or taking
int flag1 = 0;
int flag2 = 0;

int results[6] = {recvpin1, recvpin2, recvpin3, recvpin4, recvpin5, recvpin6};
int spot[6] = {0, 0, 0, 0, 0, 0};
int root[6] = {0, 0, 0, 0, 0, 0};

int ground; // Keep track the ground position
int enter_spot[6] = {0, 0, 0, 0, 0, 0};
int input;

void setup()
{
  Serial.begin(9600);

  // Start the sensor/receiver
  pinMode(IR0, INPUT);
  pinMode(IR1, INPUT);
  pinMode(recvpin1, INPUT);
  pinMode(recvpin2, INPUT);
  pinMode(recvpin3, INPUT);
  pinMode(recvpin4, INPUT);
  pinMode(recvpin5, INPUT);
  pinMode(recvpin6, INPUT);

  // Initial value 
  ground = 1;
  Slot = Total;
  

  // Start motor A
  pinMode(MotorPinA, OUTPUT); //Initiates Motor Channel A pin
  pinMode(MotorSpeedPinA, OUTPUT);//Initiates Speed Channel A pin
  pinMode(MotorBrakePinA, OUTPUT); //Initiates Brake Channel A pin

  // Start motor B
  pinMode(MotorPinB, OUTPUT); //Initiates Motor Channel B pin
  pinMode(MotorSpeedPinB, OUTPUT);//Initiates Speed Channel B pin
  pinMode(MotorBrakePinB, OUTPUT); //Initiates Brake Channel B pin

  // Set up for LCD
  lcd.init();
  lcd.backlight();
  lcd.setCursor (0, 0); // the row at which to position the cursor (with 0 being the first row)
  lcd.print(" SMART ROTARY ");
  lcd.setCursor (0, 1);
  lcd.print(" PARKING SYSTEM ");
  delay (2000);
  lcd.clear();

  // TODO:
  // Need to set up the touch screen for PARKING and TAKING to operate those functions
}

void loop()
{
  parkingCar();
  takingCar();
//  slidinggate();
  
}

void parkingCar()
{
  int irr1 = digitalRead(recvpin1); //sensor detect the car give number 0, no car give number 1
  int irr2 = digitalRead(recvpin2);
  int irr3 = digitalRead(recvpin3);
  int irr4 = digitalRead(recvpin4);
  int irr5 = digitalRead(recvpin5);
  int irr6 = digitalRead(recvpin6);
  int check = digitalRead(IR0);
  int posCheck = (irr1 * 32) + (irr2 * 16) + (irr3 * 8) + (irr4 * 4) + (irr5 * 2) + (irr6 * 1);
  // Spot 1 is available. There is no car parks at spot 1
  if ((posCheck == 63 || posCheck == 55 || posCheck == 59 || posCheck == 61 || posCheck == 62 ||
      posCheck == 47 || posCheck == 43 || posCheck == 45 || posCheck == 46 || posCheck == 60 ||
      posCheck == 39 || posCheck == 51 || posCheck == 53 || posCheck == 54 ||
      posCheck == 35 || posCheck == 57 || posCheck == 58 || posCheck == 37 || posCheck == 38 ||
      posCheck == 33 || posCheck == 49 || posCheck == 50 || posCheck == 56 ||
      posCheck == 32) && check == LOW && flag1 == 0)
  {
  if (Slot > 0 || Slot <= Total)
  {
    flag1 = 1;
    
    if (flag2 == 0)
    {
      if (ground == 1)
      {
        Slot = Slot - 1;
        delay(4000);
      }
      else
      {
        digitalWrite(MotorPinB, CCW); //Establishes forward direction of Channel B
        digitalWrite(MotorBrakePinB, LOW);   //Disengage the Brake for Channel B
        analogWrite(MotorSpeedPinB, 120);   //Spins the motor on Channel B at 15% duty cycle speed
        delay ((ground - 1) * 800);
        digitalWrite(MotorBrakePinB, HIGH); //Eengage the Brake for Channel
        delay(4000);
        
        Slot = Slot - 1;
        ground = 1;
        delay(4000);
            }
        }
  }
  }
  // Spot 2 is available
  if ((posCheck == 31 || posCheck == 27 || posCheck == 29 || posCheck == 30 ||
      posCheck == 23 || posCheck == 21 || posCheck == 22 || posCheck == 24 ||
      posCheck == 19 || posCheck == 25 || posCheck == 26 || posCheck == 20 ||
      posCheck == 17 || posCheck == 28 || posCheck == 18 ||
      posCheck == 16) && check == LOW && flag1 == 0)
  {
  if (Slot > 0 || Slot <= Total)
      {
          flag1 = 1;
          if (flag2 == 0) 
        {
            if (ground == 2)
            {
              Slot = Slot - 1;
              delay(4000);
            }
     
            if (ground == 1)
            {
              digitalWrite(MotorPinB, CW); //Establishes forward direction of Channel B
              digitalWrite(MotorBrakePinB, LOW);   //Disengage the Brake for Channel B
              analogWrite(MotorSpeedPinB, 120);   //Spins the motor on Channel B at 15% duty cycle speed
              delay (800);
              digitalWrite(MotorBrakePinB, HIGH); //Eengage the Brake for Channel
              delay(4000);
              Slot = Slot - 1;
              ground = 2;
              delay(4000);
            }
            else
            {
                digitalWrite(MotorPinB, CCW); //Establishes forward direction of Channel B
                digitalWrite(MotorBrakePinB, LOW);   //Disengage the Brake for Channel B
                analogWrite(MotorSpeedPinB, 120);   //Spins the motor on Channel B at 15% duty cycle speed
                delay ((ground - 2) * 800);
                digitalWrite(MotorBrakePinB, HIGH); //Eengage the Brake for Channel
                delay(4000);
                Slot = Slot - 1;
                ground = 2;
                delay(4000);
            }
        }
      }
  }

  // Spot 3 is available
  if ((posCheck == 15 || posCheck == 13 || posCheck == 14 ||
       posCheck == 11 || posCheck == 10 || posCheck == 12 ||
       posCheck == 9 ||
       posCheck == 8) && check == LOW && flag1 == 0)
  {
  if (Slot > 0 || Slot <= Total)
  {
    flag1 = 1;
    if (flag2 == 0)
    {
          if (ground == 3)
          {
              Slot = Slot - 1;
              delay(4000);
          }
          if (ground == 1 || ground == 2)
          {
              digitalWrite(MotorPinB, CW); //Establishes forward direction of Channel B
              digitalWrite(MotorBrakePinB, LOW);   //Disengage the Brake for Channel B
              analogWrite(MotorSpeedPinB, 120);   //Spins the motor on Channel B at 15% duty cycle speed
              delay ((3 - ground) * 800);
              digitalWrite(MotorBrakePinB, HIGH); //Eengage the Brake for Channel
              delay(4000);
              Slot = Slot - 1;
              ground = 3;
              delay(4000);
          }
          else
          {
              digitalWrite(MotorPinB, CCW); //Establishes forward direction of Channel B
              digitalWrite(MotorBrakePinB, LOW);   //Disengage the Brake for Channel B
              analogWrite(MotorSpeedPinB, 120);   //Spins the motor on Channel B at 15% duty cycle speed
              delay ((ground - 3) * 800);
              digitalWrite(MotorBrakePinB, HIGH); //Eengage the Brake for Channel
              delay(4000);
              Slot = Slot - 1;
              ground = 3;
              delay(4000);
          }
      }
  }
  } 

  // Spot 4 is avaible
  if ((posCheck == 7 || posCheck == 6 ||
       posCheck == 5 ||
       posCheck == 4) && check == LOW && flag1 == 0)
  {
  if (Slot > 0 || Slot <= Total)
  {
    flag1 = 1;
    if (flag2 == 0)
    {
          if (ground == 4)
          {
              Slot = Slot - 1;
              delay(4000);
          }
          if (ground == 1 || ground == 2 || ground == 3)
          {
              digitalWrite(MotorPinB, CW); //Establishes forward direction of Channel B
              digitalWrite(MotorBrakePinB, LOW);   //Disengage the Brake for Channel B
              analogWrite(MotorSpeedPinB, 120);   //Spins the motor on Channel B at 15% duty cycle speed
              delay ((4 - ground) * 800);
              digitalWrite(MotorBrakePinB, HIGH); //Eengage the Brake for Channel
              delay(4000);
              Slot = Slot - 1;
              ground = 4;
              delay(4000);
          }
          else
          {
              digitalWrite(MotorPinB, CCW); //Establishes forward direction of Channel B
              digitalWrite(MotorBrakePinB, LOW);   //Disengage the Brake for Channel B
              analogWrite(MotorSpeedPinB, 120);   //Spins the motor on Channel B at 15% duty cycle speed
              delay ((ground - 4) * 800);
              digitalWrite(MotorBrakePinB, HIGH); //Eengage the Brake for Channel
              delay(4000);
              Slot = Slot - 1;
              ground = 4;
              delay(4000);
          }
      }
  }
  }

  // Spot 5 is avaible
  if ((posCheck <= 3 &&
       posCheck >= 2) && check == LOW && flag1 == 0)
  {
  if (Slot > 0 || Slot <= Total)
  {
    flag1 = 1;
    if (flag2 == 0)
    {
          if (ground == 5)
          {
              Slot = Slot - 1;
              delay(4000);
          }
          if (ground == 6)
          {
              digitalWrite(MotorPinB, CCW); //Establishes forward direction of Channel B
              digitalWrite(MotorBrakePinB, LOW);   //Disengage the Brake for Channel B
              analogWrite(MotorSpeedPinB, 120);   //Spins the motor on Channel B at 15% duty cycle speed
              delay (800);
              digitalWrite(MotorBrakePinB, HIGH); //Eengage the Brake for Channel
              delay(4000);
              Slot = Slot - 1;
              ground = 5;
              delay(4000);
          }
          else
          {
              digitalWrite(MotorPinB, CW); //Establishes forward direction of Channel B
              digitalWrite(MotorBrakePinB, LOW);   //Disengage the Brake for Channel B
              analogWrite(MotorSpeedPinB, 120);   //Spins the motor on Channel B at 15% duty cycle speed
              delay ((5 - ground) * 800);
              digitalWrite(MotorBrakePinB, HIGH); //Eengage the Brake for Channel
              delay(4000);
              Slot = Slot - 1;
              ground = 5;
              delay(4000);
          }
      }
  }
  }

  // Spot 6 is available
  if (posCheck == 1 && check == LOW && flag1 == 0)
  {
  if (Slot > 0 || Slot <= Total)
  {
    flag1= 1;
    if (flag2 == 0)
    {
          if (ground == 6)
          {
              Slot = Slot - 1;
              delay(4000);
          }
          else
          {
              digitalWrite(MotorPinB, CW); //Establishes forward direction of Channel B
              digitalWrite(MotorBrakePinB, LOW);   //Disengage the Brake for Channel B
              analogWrite(MotorSpeedPinB, 120);   //Spins the motor on Channel B at 15% duty cycle speed
              delay ((6 - ground) * 800);
              digitalWrite(MotorBrakePinB, HIGH); //Eengage the Brake for Channel
              delay(4000);
              Slot = Slot - 1;
              ground = 6;
              delay(4000);
          }
      }
  }
  }
  
  if (posCheck == 0)
  {
    lcd.setCursor (0, 0);
    lcd.print("    SORRY :(    ");
    lcd.setCursor (0, 1);
    lcd.print("  Parking Full  ");
    delay (10000);
    lcd.clear();
  }

  if (flag1 == 1 && Slot > 0)
      {
          flag1 = 0;
      }

  Serial.print("Number of slot: ");
  Serial.println(Slot);
  Serial.print("Position check: ");
  Serial.println(posCheck);
  Serial.print("Current slot: ");
  Serial.println(ground);
  Serial.println("");
  delay (1000);
}
void takingCar()
{
  int irr1 = digitalRead(recvpin1); //sensor detect the car give number 0, no car give number 1
  int irr2 = digitalRead(recvpin2);
  int irr3 = digitalRead(recvpin3);
  int irr4 = digitalRead(recvpin4);
  int irr5 = digitalRead(recvpin5);
  int irr6 = digitalRead(recvpin6);
  int check = digitalRead(IR0);
  int posCheck = (irr1 * 32) + (irr2 * 16) + (irr3 * 8) + (irr4 * 4) + (irr5 * 2) + (irr6 * 1);
  if(Serial.available()>0)
  {
    input = Serial.read();
    Serial.println(input); 
  }
  
  // Retrive car at spot 1, sensor 1 detect the car as low and get # 0
  if ((posCheck == 0 || posCheck == 1 || posCheck == 2 || posCheck == 3 || posCheck == 4 ||
      posCheck == 5 || posCheck == 6 || posCheck == 7 || posCheck == 8 || posCheck == 9 ||
      posCheck == 10 || posCheck == 11 || posCheck == 12 || posCheck == 13 || posCheck == 14 ||
      posCheck == 15 || posCheck == 16 || posCheck == 17 || posCheck == 18 || posCheck == 19 ||
      posCheck == 20 || posCheck == 21 || posCheck == 22 || posCheck == 23 || posCheck == 24 ||
      posCheck == 25 || posCheck == 26 || posCheck == 27 || posCheck == 28 || posCheck == 29 ||
      posCheck == 30 || posCheck == 31) && input == 1 && flag2 == 0)
  {
  if (Slot > 0 || Slot <= Total)
  {
    flag2 = 1;
    
    if (flag1 == 0)
    {
      if (ground == 1)
      {
        Slot = Slot + 1;
        delay(4000);
      }
      else
      {
        digitalWrite(MotorPinB, CCW); //Establishes forward direction of Channel B
        digitalWrite(MotorBrakePinB, LOW);   //Disengage the Brake for Channel B
        analogWrite(MotorSpeedPinB, 120);   //Spins the motor on Channel B at 15% duty cycle speed
        delay ((ground - 1) * 800);
        digitalWrite(MotorBrakePinB, HIGH); //Eengage the Brake for Channel
        delay(4000);
        
        Slot = Slot + 1;
        ground = 1;
        delay(4000);
            }
        }
  }
  }
   // Retrive car at spot 2, sensor 2 detect the car as low and get # 0
  if ((posCheck == 0 || posCheck == 1 || posCheck == 2 || posCheck == 3 || posCheck == 4 ||
      posCheck == 5 || posCheck == 6 || posCheck == 7 || posCheck == 8 || posCheck == 9 ||
      posCheck == 10 || posCheck == 11 || posCheck == 12 || posCheck == 13 || posCheck == 14 ||
      posCheck == 15 || posCheck == 16 || posCheck == 17 || posCheck == 18 || posCheck == 19 ||
      posCheck == 20 || posCheck == 21 || posCheck == 22 || posCheck == 23 || posCheck == 24 ||
      posCheck == 25 || posCheck == 26 || posCheck == 27 || posCheck == 28 || posCheck == 29 ||
      posCheck == 30 || posCheck == 31 || posCheck == 32 || posCheck == 33 || posCheck == 34 ||
      posCheck == 35 || posCheck == 36 || posCheck == 37 || posCheck == 38 || posCheck == 39 ||
      posCheck == 40 || posCheck == 41 || posCheck == 42 || posCheck == 43 || posCheck == 44 ||
      posCheck == 45 || posCheck == 46 || posCheck == 47) && input == 2 && flag2 == 0)
  {
  if (Slot > 0 || Slot <= Total)
      {
          flag2 = 1;
          if (flag1 == 0) 
        {
            if (ground == 2)
            {
              Slot = Slot + 1;
              delay(4000);
            }
     
            if (ground == 1)
            {
              digitalWrite(MotorPinB, CW); //Establishes forward direction of Channel B
              digitalWrite(MotorBrakePinB, LOW);   //Disengage the Brake for Channel B
              analogWrite(MotorSpeedPinB, 120);   //Spins the motor on Channel B at 15% duty cycle speed
              delay (800);
              digitalWrite(MotorBrakePinB, HIGH); //Eengage the Brake for Channel
              delay(4000);
              Slot = Slot + 1;
              ground = 2;
              delay(4000);
            }
            else
            {
                digitalWrite(MotorPinB, CCW); //Establishes forward direction of Channel B
                digitalWrite(MotorBrakePinB, LOW);   //Disengage the Brake for Channel B
                analogWrite(MotorSpeedPinB, 120);   //Spins the motor on Channel B at 15% duty cycle speed
                delay ((ground - 2) * 800);
                digitalWrite(MotorBrakePinB, HIGH); //Eengage the Brake for Channel
                delay(4000);
                Slot = Slot + 1;
                ground = 2;
                delay(4000);
            }
        }
      }
  }
  // Retrive car at spot 3, sensor 3 detect the car as low and get # 0
  if ((posCheck == 0 || posCheck == 1 || posCheck == 2 || posCheck == 3 || posCheck == 4 ||
      posCheck == 5 || posCheck == 6 || posCheck == 7 || posCheck == 8 || posCheck == 9 ||
      posCheck == 10 || posCheck == 11 || posCheck == 12 || posCheck == 13 || posCheck == 14 ||
      posCheck == 15 || posCheck == 16 || posCheck == 17 || posCheck == 18 || posCheck == 19 ||
      posCheck == 20 || posCheck == 21 || posCheck == 22 || posCheck == 23 || posCheck == 24 ||
      posCheck == 25 || posCheck == 26 || posCheck == 27 || posCheck == 28 || posCheck == 29 ||
      posCheck == 30 || posCheck == 31 || posCheck == 32 || posCheck == 33 || posCheck == 34 ||
      posCheck == 35 || posCheck == 36 || posCheck == 37 || posCheck == 38 || posCheck == 39 ||
      posCheck == 40 || posCheck == 41 || posCheck == 42 || posCheck == 43 || posCheck == 44 ||
      posCheck == 45 || posCheck == 46 || posCheck == 47 || posCheck == 48 || posCheck == 49 ||
      posCheck == 50 || posCheck == 51 || posCheck == 52 || posCheck == 53 || posCheck == 54 ||
      posCheck == 55) && input == 3 && flag2 == 0)
  {
  if (Slot > 0 || Slot <= Total)
  {
    flag2 = 1;
    if (flag1 == 0)
    {
          if (ground == 3)
          {
              Slot = Slot + 1;
              delay(4000);
          }
          if (ground == 1 || ground == 2)
          {
              digitalWrite(MotorPinB, CW); //Establishes forward direction of Channel B
              digitalWrite(MotorBrakePinB, LOW);   //Disengage the Brake for Channel B
              analogWrite(MotorSpeedPinB, 120);   //Spins the motor on Channel B at 15% duty cycle speed
              delay ((3 - ground) * 800);
              digitalWrite(MotorBrakePinB, HIGH); //Eengage the Brake for Channel
              delay(4000);
              Slot = Slot + 1;
              ground = 3;
              delay(4000);
          }
          else
          {
              digitalWrite(MotorPinB, CCW); //Establishes forward direction of Channel B
              digitalWrite(MotorBrakePinB, LOW);   //Disengage the Brake for Channel B
              analogWrite(MotorSpeedPinB, 120);   //Spins the motor on Channel B at 15% duty cycle speed
              delay ((ground - 3) * 800);
              digitalWrite(MotorBrakePinB, HIGH); //Eengage the Brake for Channel
              delay(4000);
              Slot = Slot + 1;
              ground = 3;
              delay(4000);
          }
      }
  }
  } 
  // Retrive car at spot 4, sensor 4 detect the car as low and get # 0
  if ((posCheck == 0 || posCheck == 1 || posCheck == 2 || posCheck == 3 || posCheck == 4 ||
      posCheck == 5 || posCheck == 6 || posCheck == 7 || posCheck == 8 || posCheck == 9 ||
      posCheck == 10 || posCheck == 11 || posCheck == 12 || posCheck == 13 || posCheck == 14 ||
      posCheck == 15 || posCheck == 16 || posCheck == 17 || posCheck == 18 || posCheck == 19 ||
      posCheck == 20 || posCheck == 21 || posCheck == 22 || posCheck == 23 || posCheck == 24 ||
      posCheck == 25 || posCheck == 26 || posCheck == 27 || posCheck == 28 || posCheck == 29 ||
      posCheck == 30 || posCheck == 31 || posCheck == 32 || posCheck == 33 || posCheck == 34 ||
      posCheck == 35 || posCheck == 36 || posCheck == 37 || posCheck == 38 || posCheck == 39 ||
      posCheck == 40 || posCheck == 41 || posCheck == 42 || posCheck == 43 || posCheck == 44 ||
      posCheck == 45 || posCheck == 46 || posCheck == 47 || posCheck == 48 || posCheck == 49 ||
      posCheck == 50 || posCheck == 51 || posCheck == 52 || posCheck == 53 || posCheck == 54 ||
      posCheck == 55 || posCheck == 56 || posCheck == 57 || posCheck == 58 || posCheck == 59)
      && input == 4 && flag2 == 0)
  {
  if (Slot > 0 || Slot <= Total)
  {
    flag2 = 1;
    if (flag1 == 0)
    {
          if (ground == 4)
          {
              Slot = Slot + 1;
              delay(4000);
          }
          if (ground == 1 || ground == 2 || ground == 3)
          {
              digitalWrite(MotorPinB, CW); //Establishes forward direction of Channel B
              digitalWrite(MotorBrakePinB, LOW);   //Disengage the Brake for Channel B
              analogWrite(MotorSpeedPinB, 120);   //Spins the motor on Channel B at 15% duty cycle speed
              delay ((4 - ground) * 800);
              digitalWrite(MotorBrakePinB, HIGH); //Eengage the Brake for Channel
              delay(4000);
              Slot = Slot + 1;
              ground = 4;
              delay(4000);
          }
          else
          {
              digitalWrite(MotorPinB, CCW); //Establishes forward direction of Channel B
              digitalWrite(MotorBrakePinB, LOW);   //Disengage the Brake for Channel B
              analogWrite(MotorSpeedPinB, 120);   //Spins the motor on Channel B at 15% duty cycle speed
              delay ((ground - 4) * 800);
              digitalWrite(MotorBrakePinB, HIGH); //Eengage the Brake for Channel
              delay(4000);
              Slot = Slot + 1;
              ground = 4;
              delay(4000);
          }
      }
  }
  }
  // Retrive car at spot 5, sensor 5 detect the car as low and get # 0
  if ((posCheck == 0 || posCheck == 1 || posCheck == 2 || posCheck == 3 || posCheck == 4 ||
      posCheck == 5 || posCheck == 6 || posCheck == 7 || posCheck == 8 || posCheck == 9 ||
      posCheck == 10 || posCheck == 11 || posCheck == 12 || posCheck == 13 || posCheck == 14 ||
      posCheck == 15 || posCheck == 16 || posCheck == 17 || posCheck == 18 || posCheck == 19 ||
      posCheck == 20 || posCheck == 21 || posCheck == 22 || posCheck == 23 || posCheck == 24 ||
      posCheck == 25 || posCheck == 26 || posCheck == 27 || posCheck == 28 || posCheck == 29 ||
      posCheck == 30 || posCheck == 31 || posCheck == 32 || posCheck == 33 || posCheck == 34 ||
      posCheck == 35 || posCheck == 36 || posCheck == 37 || posCheck == 38 || posCheck == 39 ||
      posCheck == 40 || posCheck == 41 || posCheck == 42 || posCheck == 43 || posCheck == 44 ||
      posCheck == 45 || posCheck == 46 || posCheck == 47 || posCheck == 48 || posCheck == 49 ||
      posCheck == 50 || posCheck == 51 || posCheck == 52 || posCheck == 53 || posCheck == 54 ||
      posCheck == 55 || posCheck == 56 || posCheck == 57 || posCheck == 58 || posCheck == 59 ||
      posCheck == 60 || posCheck == 61) && input == 5 && flag2 == 0)
  {
  if (Slot > 0 || Slot <= Total)
  {
    flag1 = 1;
    if (flag2 == 0)
    {
          if (ground == 5)
          {
              Slot = Slot + 1;
              delay(4000);
          }
          if (ground == 6)
          {
              digitalWrite(MotorPinB, CCW); //Establishes forward direction of Channel B
              digitalWrite(MotorBrakePinB, LOW);   //Disengage the Brake for Channel B
              analogWrite(MotorSpeedPinB, 120);   //Spins the motor on Channel B at 15% duty cycle speed
              delay (800);
              digitalWrite(MotorBrakePinB, HIGH); //Eengage the Brake for Channel
              delay(4000);
              Slot = Slot + 1;
              ground = 5;
              delay(4000);
          }
          else
          {
              digitalWrite(MotorPinB, CW); //Establishes forward direction of Channel B
              digitalWrite(MotorBrakePinB, LOW);   //Disengage the Brake for Channel B
              analogWrite(MotorSpeedPinB, 120);   //Spins the motor on Channel B at 15% duty cycle speed
              delay ((5 - ground) * 800);
              digitalWrite(MotorBrakePinB, HIGH); //Eengage the Brake for Channel
              delay(4000);
              Slot = Slot + 1;
              ground = 5;
              delay(4000);
          }
      }
  }
  }
  // Retrive car at spot 6, sensor 6 detect the car as low and get # 0
  if ((posCheck == 0 || posCheck == 1 || posCheck == 2 || posCheck == 3 || posCheck == 4 ||
      posCheck == 5 || posCheck == 6 || posCheck == 7 || posCheck == 8 || posCheck == 9 ||
      posCheck == 10 || posCheck == 11 || posCheck == 12 || posCheck == 13 || posCheck == 14 ||
      posCheck == 15 || posCheck == 16 || posCheck == 17 || posCheck == 18 || posCheck == 19 ||
      posCheck == 20 || posCheck == 21 || posCheck == 22 || posCheck == 23 || posCheck == 24 ||
      posCheck == 25 || posCheck == 26 || posCheck == 27 || posCheck == 28 || posCheck == 29 ||
      posCheck == 30 || posCheck == 31 || posCheck == 32 || posCheck == 33 || posCheck == 34 ||
      posCheck == 35 || posCheck == 36 || posCheck == 37 || posCheck == 38 || posCheck == 39 ||
      posCheck == 40 || posCheck == 41 || posCheck == 42 || posCheck == 43 || posCheck == 44 ||
      posCheck == 45 || posCheck == 46 || posCheck == 47 || posCheck == 48 || posCheck == 49 ||
      posCheck == 50 || posCheck == 51 || posCheck == 52 || posCheck == 53 || posCheck == 54 ||
      posCheck == 55 || posCheck == 56 || posCheck == 57 || posCheck == 58 || posCheck == 59 ||
      posCheck == 60 || posCheck == 61 || posCheck == 62) && input == 6 && flag2 == 0)
  {
  if (Slot > 0 || Slot <= Total)
  {
    flag2= 1;
    if (flag1 == 0)
    {
          if (ground == 6)
          {
              Slot = Slot + 1;
              delay(4000);
          }
          else
          {
              digitalWrite(MotorPinB, CW); //Establishes forward direction of Channel B
              digitalWrite(MotorBrakePinB, LOW);   //Disengage the Brake for Channel B
              analogWrite(MotorSpeedPinB, 120);   //Spins the motor on Channel B at 15% duty cycle speed
              delay ((6 - ground) * 800);
              digitalWrite(MotorBrakePinB, HIGH); //Eengage the Brake for Channel
              delay(4000);
              Slot = Slot + 1;
              ground = 6;
              delay(4000);
          }
      }
  }
  }
  if (posCheck == 63)
  {
    lcd.setCursor (0, 0);
    lcd.print("    SORRY :(    ");
    lcd.setCursor (0, 1);
    lcd.print("  No car in the parking lot  ");
    delay (10000);
    lcd.clear();
  }

  if (flag2 == 1 && Slot > 0)
      {
          flag2 = 0;
      }

  Serial.print("Number of slot: ");
  Serial.println(Slot);
  Serial.print("Position check: ");
  Serial.println(posCheck);
  Serial.print("Current slot: ");
  Serial.println(ground);
  Serial.println("");
  delay (1000);
}
  
