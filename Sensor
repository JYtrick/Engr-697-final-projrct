void setup() {
  
Serial.begin(9600);
pinMode(8, INPUT); // set pin as input

}

void loop() {
  int detect = digitalRead(8); // read cars status and store
  if (detect == LOW) {
    Serial.println("car on the parking lot");
  }else{
    
    Serial.println("Parking available");
  }
  delay(300);
}
